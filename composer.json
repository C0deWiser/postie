{
    "name": "codewiser/postie",
    "description": "Subscription management Laravel package",
    "keywords": ["subscription", "notifications", "laravel"],
    "license": "MIT",
    "suggest": {
        "codewiser/laravel-notifications": "Brings notification message contract"
    },
    "authors": [
        {
            "name": "Michael Pogrebnikov",
            "email": "pm@101media.ru"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Codewiser\\Postie\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Codewiser\\Postie\\PostieServiceProvider"
            ]
        }
    }
}
